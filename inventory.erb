[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.4.1
openshift_image_tag=v1.4.1
openshift_public_hostname=console.virtualflybrain.org
openshift_master_default_subdomain=apps.console.virtualflybrain.org
openshift_hosted_metrics_deploy=false
openshift_master_overwrite_named_certificates=true
oauthConfig:
  
  identityProviders:
  - name: github 
    challenge: false 
    login: true 
    provider:
      apiVersion: v1
      kind: GitHubIdentityProvider
      clientID: ... 
      clientSecret:

[masters]
console.virtualflybrain.org openshift_schedulable=true

[nodes]
console.virtualflybrain.org openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
